@model IEnumerable<ProyectoClub.Models.SedeActividad>

@{
    ViewData["Title"] = "Asociaciones Sede - Actividad";
}

<h2>Asociaciones Sede - Actividad</h2>

<table class="table">
    <thead>
        <tr>
            <th>Sede</th>
            <th>Actividad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Sede.Nombre</td>
                <td>@item.Actividad.Nombre</td>
            </tr>
        }
    </tbody>
</table>

<div class="d-flex mb-3">
    <a asp-action="Create" class="btn btn-success">Nueva Asociación</a>
    <a asp-controller="Admin" asp-action="Dashboard" class="btn btn-secondary">Volver al Dashboard</a>
</div>

